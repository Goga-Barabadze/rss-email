      "binding": "FEEDS_KV",
      "id": "REPLACE_WITH_PRODUCTION_KV_ID",
      "preview_id": "REPLACE_WITH_PREVIEW_KV_ID"
    }
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_id": "d609b147-9a57-4967-81e6-0f47fdf8dbcb",
      "database_name": "database"
    }
  ],
  "vars": {
    "MAILGUN_DOMAIN": "mg.example.com",
    "MAILGUN_FROM": "RSS Worker <rss@example.com>",
    "MAILGUN_RECIPIENT": "gobarabadze@gmail.com"
  },
  "observability": {
    "enabled": true
  }
}
